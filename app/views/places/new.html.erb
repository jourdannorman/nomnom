<div class="booyah-box col-10 offset-1">
	<h1>Enter Your Place Details</h1>
	<br />
	<%= simple_form_for @place do |f| %>
		<%= f.input :name, label: 'Restaurant Name' %>
		<%= f.input :address %>
		<%= f.input :description %>
		<br />
		<%= f.submit 'Create', class: 'btn btn-outline-primary btn-lg btn-block' %>
	<% end %>
</div>